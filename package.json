{
  "name": "js-full-stack-dev-env",
  "version": "1.2.2",
  "description": "A full stack dev environment based on node js",
  "main": "index.html",
  "scripts": {
    "prod": "PORT=8000 node server/server.js",
    "dev": "concurrently --kill-others \"npm run dev_server\" \"npm run webpack\"",
    "dev_server": "PORT=8000 nodemon --inspect -e js,html,css,scss --ignore client/js --ignore client/css --ignore client/dist server/server.js ",
    "webpack": " node_modules/.bin/webpack --optimize-minimize  --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.wvrgroup.internal:cpullar/js-full-stack-dev-env.git"
  },
  "author": "cpullar@homeaway.co.uk",
  "license": "ISC",
