    "grunt-shell": "^2.1.0",
    "handlebars": "^4.0.6",
    "hapi": "^16.1.0",
    "hapi-rethinkdb": "^2.0.1",
    "inert": "^4.1.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "rethinkdb": "^2.3.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "vision": "^4.1.1",
